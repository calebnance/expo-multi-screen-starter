{
  "expo": {
    "name": "expo-multi-screen-starter",
    "description": "react native multi screen application starter with expo",
    "slug": "expo-multi-screen-starter",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "version": "0.0.2",
    "platforms": ["android", "ios", "web"],
    "orientation": "portrait",
    "primaryColor": "#1d2d3a",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#1d2d3a",
      "image": "src/assets/splash.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": ["src/assets/**/*"],
    "androidStatusBar": {
      "backgroundColor": "#1d2d3a"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "metatags": {
        "author": "caleb nance"
      },
      "twitter": {
        "card": "summary",
        "title": "Multi Screen Starter - Web",
        "description": "multi screen starter in web",
        "site": "https://calebnance.com",
        "creator": "caleb nance"
      }
    }
  }
}
